.readme-container {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    padding: 16rpx;
}

.readme-header {
    width: 100%;
    height: 120rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: bold;
}

.readme-content {
    font-size: 32rpx;
}